<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <HediffDef>
    <defName>VESWW_IncreasedStats</defName>
    <label>increased stats</label>
    <description>Increased stats as a counterpart of Winston</description>
    <hediffClass>HediffWithComps</hediffClass>
    <initialSeverity>1</initialSeverity>
    <everCurableByItem>false</everCurableByItem>
    <stages>
      <li>
        <becomeVisible>false</becomeVisible>
        <statFactors>
          <InjuryHealingFactor>2</InjuryHealingFactor>
          <WorkSpeedGlobal>1.5</WorkSpeedGlobal>
        </statFactors>
        <capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>0.50</offset>
					</li>
          <li>
						<capacity>Consciousness</capacity>
						<offset>0.1</offset>
					</li>
          <li>
						<capacity>Moving</capacity>
						<offset>0.25</offset>
					</li>
				</capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>